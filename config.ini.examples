[config]
iniFile = dumpconf.ini
logfile = ./dump.log
dest_path = /usr/pydump_dev/backup/
pg_dump = pg_dump
pg_dump_options = -Fc
archiver = tar
archiver_options = cf
archiver_ext = .tar
compress_app = bzip2
compress_ext = .bz
dump_ext = .dump
nagios_server = nagios.server
send_nsca = /usr/sbin/send_nsca
